GET {{baseUrl}}/users/1
{{json}}

###
# Atualizar um usuário pelo ID
PUT {{baseUrl}}/users/4
{{json}}

{
  "name": "Marco Atualizado",
  "email": "marco.new@example.com"
}

###
# Deletar um usuário pelo ID
DELETE {{baseUrl}}/users/4
{{json}}

